{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "build",
            "command": ".stbuild/build.cmd",
            "type": "process",
            "args": [
                "--Target Compile",
                "--Variant Debug"
            ],
            "problemMatcher": "$msCompile",
            "group": {
                "kind": "build",
                "isDefault": true
            }
        },
        {
            "label": "pack",
            "command": ".stbuild/build.cmd",
            "type": "process",
            "args": [
                "--Target Pack",
                "--Variant Release"
            ],
            "problemMatcher": "$msCompile",
            "group": {
                "kind": "build",
                "isDefault": false
            }
        },
        {
            "label": "clean release",
            "command": ".stbuild/build.cmd",
            "type": "process",
            "args": [
                "--Target Clean",
                "--Variant Release"
            ],
            "problemMatcher": "$msCompile",
            "group": {
                "kind": "build",
                "isDefault": false
            }
        },
        {
            "label": "clean debug",
            "command": ".stbuild/build.cmd",
            "type": "process",
            "args": [
                "--Target Clean",
                "--Variant Debug"
            ],
            "problemMatcher": "$msCompile",
            "group": {
                "kind": "build",
                "isDefault": false
            }
        },
        {
            "label": "build-op-seperately",
            "command": "dotnet",
            "type": "process",
            "args": [
                "build",
                "${workspaceFolder}/project/main/PunchingOperationExtension.csproj",
                "/property:GenerateFullPaths=true",
                "/consoleloggerparameters:NoSummary;ForceNoAlign"
            ],
            "problemMatcher": "$msCompile",
            "group": {
                "kind": "build",
                "isDefault": false
            }
        },
        {
            "label": "build-stbuild",
            "command": "dotnet",
            "type": "process",
            "args": [
                "build",
                "${workspaceFolder}/.stbuild/build/stbuild.csproj",
                "/property:GenerateFullPaths=true",
                "/consoleloggerparameters:NoSummary;ForceNoAlign"
            ],
            "problemMatcher": "$msCompile"
        },
        {
            "label": "publish",
            "command": "dotnet",
            "type": "process",
            "args": [
                "publish",
                "${workspaceFolder}/.stbuild/build/stbuild.csproj",
                "/property:GenerateFullPaths=true",
                "/consoleloggerparameters:NoSummary;ForceNoAlign"
            ],
            "problemMatcher": "$msCompile"
        },
        {
            "label": "watch",
            "command": "dotnet",
            "type": "process",
            "args": [
                "watch",
                "run",
                "--project",
                "${workspaceFolder}/.stbuild/build/stbuild.csproj"
            ],
            "problemMatcher": "$msCompile"
        }
    ]
}